{
  // I want to use babel-eslint for parsing!
  "parser": "babel-eslint",
  "env": {
    // I write for browser
    "browser": true,
    // in CommonJS
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "google", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules" : {
    "no-return-assign": [0],
    "linebreak-style": ["warn", "windows"]
  },
  "globals": {
    "_": true,
    "$": true,
    "Utils": true
  }
}